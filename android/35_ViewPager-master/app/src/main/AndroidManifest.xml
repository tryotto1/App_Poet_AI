<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hongdroid.viewpagerexample">
    <!-- 서버 접근용 -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ExtraTabs.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".ExtraTabs.SignupActivity" />
        <activity android:name=".ExtraTabs.ShowWritingActivity" />
        <activity android:name=".ExtraTabs.FollowingActivity" />
        <activity android:name=".ExtraTabs.LikeActivity" />
        <activity android:name=".ExtraTabs.MyWritingsActivity" />

        <activity android:name=".ExtraTabs.WriteActivity" />
        <activity android:name=".MainActivity"></activity>

        <activity android:name=".ExtraTabs.LoginActivity">
        </activity>
    </application>

</manifest>